{
    "name": "tanyudii/laravel-exception",
    "description": "The Package Exception Notification through Telegram Bot API.",
    "license": "MIT",
    "authors": [
        {
            "name": "Yudi Hertanto",
            "email": "y.hertanto17@gmail.com"
        }
    ],
    "homepage": "https://github.com/tanyudii/laravel-exception",
    "keywords": ["Laravel", "LaravelException"],
    "require": {
        "illuminate/support": "~7|~8",
        "westacks/telebot": "^1.12"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "tanyudii\\LaravelException\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tanyudii\\LaravelException\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "tanyudii\\LaravelException\\LaravelExceptionServiceProvider"
            ],
            "aliases": {
                "LaravelException": "tanyudii\\LaravelException\\Facades\\LaravelException"
            }
        }
    }
}
